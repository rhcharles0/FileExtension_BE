generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model FileExtension {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  isAllowed Boolean  @default(true) // 이 확장자가 업로드 가능한지 여부
  createdAt DateTime @default(now())
}
